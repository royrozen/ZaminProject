<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <sectionGroup name="elmah">
      <section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
      <section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
      <section name="errorMail" requirePermission="false" type="Elmah.ErrorMailSectionHandler, Elmah" />
      <section name="errorFilter" requirePermission="false" type="Elmah.ErrorFilterSectionHandler, Elmah" />
    </sectionGroup>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <appSettings>
    <add key="serverUrl" value="http://zamin/" />
    <add key="serverDirectory" value="G:\Projects\Zamin\Zamin.Server\" />
        <!--************** G *************-->
    <!--<add key="serverDirectory" value="E:\Projects\Zamin\Zamin.Server\" />-->

    <add key="owin:AutomaticAppStartup" value="false" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    
    <!--Directories-->
    <add key="lessonPlansPresentationsFolder" value="Content/Presentations"/>
    <add key="lessonPlansWrittenFolder" value="Content/Written"/>
    <add key="coursesFolder" value="Content/Courses"/>
    <add key="postersFolder" value="Content/Posters"/>
    <add key="articlesFolder" value="Content/Articles"/>
    <add key="activitiesFolder" value="Content/Activities"/>
    <add key="galleryImagesFolder" value="Content/GalleryImages"/>
    <add key="zipFolder" value="Content/Zip"/>
    

    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.IgnoreDefaultRoute" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.allowedUsers" value="*" />
    <add key="elmah.mvc.route" value="elmah" />
    <add key="elmah.mvc.UserAuthCaseSensitive" value="true" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1"/>
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <globalization uiCulture="en-GB" culture="en-GB" />
    <authentication mode="Forms">
      <forms loginUrl="~/Account/Login" timeout="6048000" />
    </authentication>
    <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
    <membership defaultProvider="CodeFirstMembershipProvider">
      <providers>
        <add name="CodeFirstMembershipProvider" type="Zamin.Repositories.CFMembership.CodeFirstMembershipProvider" connectionStringName="DataContext" />
      </providers>
    </membership>
    <!--<roleManager enabled="true" defaultProvider="CodeFirstRoleProvider">
      <providers>
        <clear />
        <add name="CodeFirstRoleProvider" type="Zamin.Repositories.CFMembership.CodeFirstRoleProvider" connectionStringName="DataContext" />
      </providers>
    </roleManager>-->
    <!--^^^^^^^^^^^^^^^^^^^^^^^^^^^^-->
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
