(function () {
  'use strict';

  angular
    .module('tags')
    .config(config);

  function config($stateProvider) {
    $stateProvider
      .state('tags', {
        url: '/tags',
        templateUrl: 'tags/tags.tpl.html',
        controller: 'TagsCtrl',
        controllerAs: 'tags'
      });
  }
}());
