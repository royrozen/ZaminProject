/* hebrew */
@font-face {
  font-family: 'Varela Round';
  font-style: normal;
  font-weight: 400;
  src: url('../../fonts/VarelaRound-Regular.ttf') format('truetype');
}
body {
  font-family: 'Varela Round';
}
header {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  height: 10vh;
  z-index: 999;
}
.md-dialog-container {
  z-index: 9999;
}
.main-view {
  padding-top: 12vh;
  background-color: #ECEFF1;
  min-height: 82vh;
}
.subMenu{
      margin-top: 72px;
}
div,
md-card,
md-menu,
span {
  outline: none;
}
.logo {
  text-align: right;
  cursor: pointer;
  img {
    max-height: 4vw;
  }
}
md-toolbar.md-hue-3.md-warn {
  background-color: #CFD8DC;
  padding: 0.5vw;
  height: 12vh;
  min-height: 0;
  max-height: 10vw;
  cursor: pointer;
  > div,
  md-menu {
    opacity: 0.8;
    color: #78909C;
    margin-left: 1vw;
    margin-right: 1vw;
  }
   > div:hover,
  > div.active,
  md-menu.active,
  md-menu:hover {
    opacity: 1;
    color: white;
  }
}
.main-header {
  align-items: center;
  justify-content: flex-start;
}
footer {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 6vh;
  background-color: #455A64;
  z-index: 999;
  align-items: center;
  > a {
    max-height: 2vw;
    margin-left: 1vw;
    img {
      max-height: 2vw;
    }
  }
}
md-toolbar.md-hue-3.md-warn > div.login-btn{
background-color: orange;
color: white;
font-size: 22px;
border-radius: 5px;
}
md-toolbar.md-hue-3.md-warn > div.login-btn:hover{
  color: orange;
}
.login-dialog{
  text-align: right;
  background-color: #cfd8dc;
  padding: 20px;
}
