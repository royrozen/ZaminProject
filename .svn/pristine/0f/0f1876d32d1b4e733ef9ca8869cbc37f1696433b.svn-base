.md-datepicker-calendar-pane.md-pane-open{
  z-index: 999;
}

input[type=date]::-webkit-inner-spin-button {
    -webkit-appearance: none;
    display: none;
}

input[type=date]::-webkit-calendar-picker-indicator {
    -webkit-appearance: none;
    display: none;
}

md-input-container label:not(.md-container-ignore) {
  left:auto !important;
}

.combos .form-group{
  display: flex;
  align-items: center;
}
.combos md-card{
 background-color:  rgb(250,250,250);
 box-shadow: 0px 1px 0px 2px rgba(0, 0, 0, 0.2), 0px 1px 1px 0px rgba(0, 0, 0, 0.14), 0px 2px 1px -1px rgba(0, 0, 0, 0.12);
 padding: 2%;

}
.no-margin{
  margin: 0;
}
