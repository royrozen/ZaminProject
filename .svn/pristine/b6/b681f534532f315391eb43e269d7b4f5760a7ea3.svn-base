#media iframe {
  /*width: 25vw;*/
  width: 90%;
  height: 20vw;
}
#videos {
  direction: ltr;
}
.iframe-container {
  position: relative;
  width: 60%;
  height: 60%
}
.iframe-containerArticle {
  position: relative;
  width: 50%;
  height: 50%
}
.iframe-containerGallery {
  position: relative;
  width: 60%;
  height: 60%
}
.iframe-overlay {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.iframe-btn {
  position: absolute;
  top: 35%;
  right: 40%;
  width: 5vw;
  height: 5vw;
  cursor: pointer;
}
